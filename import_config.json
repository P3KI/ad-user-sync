{
    "input_file": "users.json",
    "base_path": "CN=Users,DC=mydomain,DC=com",
    "managed_user_path": "CN=P3KI Managed",
    "group_map": {
        "CN=t-Administrators" : "CN=p-Administrators",
        "CN=t-Operators"      : "CN=p-Operators",
        "CN=t-Viewers"        : "CN=p-Viewers",
        "*"                   : "CN=p-Managed"
    },
    "restricted_groups": ["CN=p-Administrators"],
    "prefix_common_names": "P3KI ",
    "default_expiration": "P1M2D",
    "rejected_actions": "rejections.json",
    "log_level": "DEBUG",
    "heartbeat_interval": 2
}